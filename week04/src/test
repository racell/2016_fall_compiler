int a = 1;
int b = 100;
int list[10];
void main(){
    int i;
    int j;
    int k;
    int rem;
    int sum;
    i = 2;
    a = 8;
    list[3] = 5;
    while(i <= 500) {
        sum = 0;
        k = i/2;
        j = i;
        while ( j <= k) {
            rem = i%j;
            if (rem == 0) {
                sum = sum + j;
                ++j;
            }
        }
        if (i == sum)
            write(i);
        ++ i;
    }
}