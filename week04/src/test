int a = 1;
int b = 100;
int list[10];
void main(){
    int i;
    int j;
    int k;
    int rem;
    int sum;
    int o;
    i = test(list, 3);
    a = 8;
    list[3] = 5;
    o = list[3];
    while(i <= 500) {
        sum = 0;
        k = i/2;
        j = 1;
        while ( j <= k) {
            rem = i%j;
            if (rem == 0) {
                sum = sum + j;
            }
            ++j;
        }
        if (i == sum) write(i);
        ++ i;
    }
    write(o);
    write(a);
    write(list[3]);
}

int test(int array[], int t) {
    int x = 2;
    array[3] = 4;
    write(array[3]);
    return x;
}